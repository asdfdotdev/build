  "description": "Better builds, building better websites",
  "main": "index.js",
  "bin": {
    "flickerbox-build": "bin/flickerbox-build.js"
  },
  "scripts": {
    "production": "webpack --config=config/webpack.prod.js --info-verbosity verbose --progress && npm run delete-maps",
    "development": "webpack --config=config/webpack.dev.js --info-verbosity verbose --progress",
    "development-dashboard": "webpack-dashboard -- npm run development",
    "delete-maps": "find ./ -name '*.map' -delete",
    "help": "flickerbox-build -- help",
    "version-check": "flickerbox-build -- version-check",
    "commands": "npm run help",
    "dev": "npm run development",
    "dev-dashboard": "npm run development-dashboard",
    "prod": "npm run production",
    "watch": "npm run development"
  },
